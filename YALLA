    -- //  Mutation multipliers
    local MutationMultipliers = {
        ["Gold"] = 1.25,
        ["Diamond"] = 1.50,
        ["Rainbow"] = 10,
        ["Candy"] = 4,
        ["Lava"]  = 6,
        ["Bloodrot"]  = 2,
